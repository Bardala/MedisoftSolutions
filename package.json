{
  "name": "clinic",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "frontend": "cd frontend && npm start",
    "frontend-build": "cd frontend && npm run build",
    "backend": "cd backend && mvn spring-boot:run",
    "backend-dev": "cd backend && cross-env SPRING_PROFILES_ACTIVE=dev mvn spring-boot:run",
    "backend-prod": "cd backend && cross-env SPRING_PROFILES_ACTIVE=prod mvn spring-boot:run",
    "backend-clean": "cd backend && mvn clean",
    "backend-build-dev": "cd backend && mvn clean package -Dspring.profiles.active=dev",
    "backend-build-prod": "cd backend && mvn clean package -Dspring.profiles.active=prod",
    "backend-build-prod-skip-testing": "cd backend && mvn clean package -DskipTests -Dspring.profiles.active=prod",
    "test:backend": "cd backend && mvn test",
    "start": "concurrently \"npm run frontend\" \"npm run backend-dev\""
  },
  "author": "islam bardala",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "rimraf": "^6.0.1"
  }
}
